{
    "contentTypes": [
        {
            "name": "Trade (Personal)",
            "label": "Personal Trade",
            "file": "content/trades/{year}-{month}-{day}-{slug}/index.md",
            "create": true,
            "fields": [
                {
                    "label": "Title",
                    "name": "title",
                    "type": "string",
                    "required": true
                },
                {
                    "label": "Date (full)",
                    "name": "date",
                    "type": "datetime",
                    "required": true
                },
                {
                    "label": "Time",
                    "name": "time",
                    "type": "string",
                    "hint": "HH:MM (optional)"
                },
                {
                    "label": "Day of week",
                    "name": "day",
                    "type": "string"
                },
                {
                    "label": "Instrument",
                    "name": "instrument",
                    "type": "select",
                    "options": [
                        "DAX",
                        "DOW",
                        "NASD",
                        "USDJPY",
                        "EURUSD",
                        "GBPUSD",
                        "GOLD"
                    ],
                    "required": true
                },
                {
                    "label": "Direction",
                    "name": "direction",
                    "type": "select",
                    "options": [
                        "Long",
                        "Short"
                    ]
                },
                {
                    "label": "Setup",
                    "name": "setup",
                    "type": "textarea"
                },
                {
                    "label": "Confluence Entry / SL",
                    "name": "confluence",
                    "type": "textarea"
                },
                {
                    "label": "Target",
                    "name": "target",
                    "type": "textarea"
                },
                {
                    "label": "Notes",
                    "name": "notes",
                    "type": "markdown"
                },
                {
                    "label": "Rules Followed?",
                    "name": "rules_followed",
                    "type": "select",
                    "options": [
                        "yes",
                        "no"
                    ],
                    "default": "yes"
                },
                {
                    "label": "Thumbnail (card)",
                    "name": "thumbnail",
                    "type": "image",
                    "note": "Saved inside the bundle"
                },
                {
                    "label": "Main image (screenshot)",
                    "name": "image",
                    "type": "image",
                    "note": "Saved inside the bundle"
                },
                {
                    "label": "Tags",
                    "name": "tags",
                    "type": "list",
                    "items": {
                        "type": "string"
                    }
                }
            ]
        },
